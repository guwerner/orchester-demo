{
  "_version": "1.49.0",
  "sap.app": {
    "id": "musican",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "type": "application",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "i18n": "i18n/i18n.properties",
    "dataSources": {
      "MainService": {
        "uri": "musican/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "Musican-manage": {
          "signature": {
            "parameters": {
              "Musican.ID": {
                "renameTo": "ID"
              }
            },
            "additionalParameters": "allowed"
          },
          "semanticObject": "Musican",
          "action": "display",
          "title": "{{appTitle}}",
          "info": "{{appInfo}}",
          "subTitle": "{{appSubTitle}}",
          "icon": "sap-icon://course-book",
          "indicatorDataSource": {
            "dataSource": "MusicanService",
            "path": "Musican/$count",
            "refresh": 1800
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "fullWidth": false,
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.140.0",
      "libs": {
        "sap.fe.templates": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      },
      "": {
        "dataSource": "MainService",
         "preload": true,
        "settings": {
         "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "groupProperties": {
            "default": {
              "submit": "Auto"
            }
          }
        }
      }
    },
    "routing": {
      "routes": [
        {
          "pattern": ":?query:",
          "name": "MusicanList",
          "target": "MusicanList"
        },
        {
          "pattern": "Musican({key}):?query:",
          "name": "MusicanDetails",
          "target": "MusicanDetails"
        },
        {
        "pattern": "Musican({key})/to_band({key2}):?query:",
        "name": "MusianMembersObjectPage",
        "target": "MusianMembersObjectPage"
        }
      ],
      "targets": {
        "MusicanList": {
          "type": "Component",
          "id": "MusicanList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "entitySet": "Musican",
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.SelectionFields": {
                  "useSemanticDateRange": true
                }
              },
              "variantManagement": "Page",
              "initialLoad": "Enabled",
              "navigation": {
                "Musican": {
                  "detail": {
                    "route": "MusicanDetails"
                  }
                }
              }
            }
          }
        },
        "MusicanDetails": {
          "type": "Component",
          "id": "MusicanDetailsList",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "Musican",
              "navigation": {
                "to_band": {
                  "detail": {
                    "route": "MusianMembersObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable",
                    "personalization": {
                      "column": true,
                      "sort": false
                    },
                    "creationMode": {
                      "name": "Inline",
                      "createAtEnd": true
                    },
                    "editableHeaderContent": false,
                    "showRelatedApps": true,
                    "enableMassEdit": true,
                    "enableExport": true
                  }
                }
              }
            }
          }
        },
        "MusianMembersObjectPage": {
          "type": "Component",
          "id": "MusianMembersObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "entitySet": "Band2Musicans",
              "editableHeaderContent": false
            }
          }
        }
      }
     },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
