{
  "$schema": "https://www.schemastore.org/tsconfig",
  "_version": "22.0.0",

    "compilerOptions": {
        "target": "ES2022",
        "module": "nodenext",
           "lib": ["es2024", "ESNext.Array", "ESNext.Collection", "ESNext.Iterator"],
        "rootDir": "./",
        "baseUrl": "./",
        "allowJs": true,
        "esModuleInterop": true,
        "inlineSources": true,
        "inlineSourceMap": true,
        "moduleResolution": "nodenext",
        "paths": {
            "#cds-models/*": [
                "./@cds-models/*"
            ]
        },
        "types": [
            "jest",
            "node",
            "@cap-js/cds-types",
        ],
    },
    "include": [
        "./srv/**/*"
    ]
}
